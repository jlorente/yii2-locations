    "keywords": ["yii2", "location", "country", "region", "city", "countries", "regions", "cities", "locations"],
    "homepage": "https://github.com/jlorente/yii2-locations",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Lorente Martin",
            "email": "jose.lorente.martin@gmail.com"
        }
    ],
    "require": {
        "jlorente/yii2-google-maps-library": "*",
        "kartik-v/yii2-widget-depdrop": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "jlorente\\location\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
